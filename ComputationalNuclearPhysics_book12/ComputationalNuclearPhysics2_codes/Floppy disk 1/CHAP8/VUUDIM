C     -------------  VUUDIM.FOR ---------------------
C      SHOULD BE INCLUDED AFTER VUUPRM
C
      PARAMETER (NPEVMA=600)
C       MAX. TOTAL PARTICLE NUMBER OF EVENT = BARYONS +PIONS
      PARAMETER (NTOTMA=NPEVMA*NSIMUL)
C       NSIMUL=15 DEFINED IN VUUPRM -> NTOTMA=9000
      PARAMETER (NPIONI=100)
C       MAX. PION NUMBER OF EVENT
      DIMENSION X(NTOTMA),Y(NTOTMA),Z(NTOTMA),
     &          PX(NTOTMA),PY(NTOTMA),PZ(NTOTMA),
     &          EM(NTOTMA),ITYP(NTOTMA),DECAY(NTOTMA)
      DIMENSION AX(NTOTMA),AY(NTOTMA),AZ(NTOTMA),
     &          FX(NTOTMA),FY(NTOTMA),FZ(NTOTMA),
     &          DENSPP(NTOTMA),XYZSQ(NTOTMA)
C CH  LOGICAL LOCK(NTOTMA)
      DIMENSION NPION(NSIMUL)
C CH  INTEGER*4 LOCX(NTOTMA),LOCY(NTOTMA),LOCZ(NTOTMA)
      COMMON /PARTBL/NT,NPION,TIME,DT,KSIMUL,IOFSET,NPIONM,
     &                     X,Y,Z,PX,PY,PZ,EM,ITYP,DECAY,
C CH &                     LOCK,LOCX,LOCY,LOCZ,
     &                     AX,AY,AZ,FX,FY,FZ
      COMMON /NOINFB/ DENSPP,XYZSQ
C
C  ...  STORAGE TO STORE LAST COLLISION PARTNER AND COLL.NUM.
C
      COMMON /LASTBK/LASTCL(NTOTMA),NCLCNT(NTOTMA)
C
C  ...  PARAMETERS AND COMMON FOR DENSITY SUBROUTINE
C
C
C ...  VERSION>5, COLLISION MATRIX
C
      PARAMETER (NUMEMA=2500)
      DIMENSION I1C(NUMEMA),I2C(NUMEMA),
     *          CTIME(NUMEMA),CS(NUMEMA),
     *          CSIG(NUMEMA),CSIGEL(NUMEMA)
      COMMON /COLBLK/NUMENT,I1C,I2C,CTIME,CS,CSIG,CSIGEL
