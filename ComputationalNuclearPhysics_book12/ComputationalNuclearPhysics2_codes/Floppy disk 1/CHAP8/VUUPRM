C    ------------  VUUPRM.FOR ----------------------
C
C ...  PARAMETERS AND COMMON BLOCKS FOR PROGRAM  VUU
C
      PARAMETER (MAXTYP=45,MXCHAN=5,NUMPRT=9)
      PARAMETER (PI=3.1415927,PI2=2.*PI,PISQ=PI*PI)
      PARAMETER (HC=1.2398)
C                2*PI*H-BAR*C
C
C ...  NUMBER OF SIMULTANEOUS RUNS
C
      PARAMETER (NSIMUL=15)
C
C ...  PARTICLE MASSES IN GEV
C
      PARAMETER (EMDELT=1.232,EMNUC=.938,EMPION=.138)
      PARAMETER (S12M22=EMNUC**2+EMPION**2)
      PARAMETER (P12M22=EMNUC**2*EMPION**2)
C
C ...  WIDTH OF THE DELTA RESONANCE
C
      PARAMETER (WIDDLT=.120)
C
C ...  PARAMETERS FOR THE MASS-DEPENDENCE OF THE DELTA DECAY WIDTH
C
      PARAMETER (PSCAL1=.238,PSCAL2=.318,P0REF=.227)
C
C ...  LOWEST KINETIC ENERGY ALLOWED IN TWO-BODY EXIT CHANNEL
C
      PARAMETER (EKININ=.05)
C
      PARAMETER (CONST=2.*(EMNUC+EMPION-EMDELT)/
     #   WIDDLT)
C
C ...  UPPER LIMIT OF DELTA RESONANCE SCATTERING
C
      PARAMETER (DLTLIM=3.00)
C
      COMMON /CNTBK/ICOUNT(MXCHAN,MAXTYP,NSIMUL)
      COMMON /CNSTBK/CONST1,CONST2
C
      DIMENSION SEACUT(MAXTYP)
      DIMENSION LOCCUT(MAXTYP)
      COMMON /PARMBL/DA(22),SEACUT,LOCCUT
C
      PARAMETER (NUMSWI=12)
      LOGICAL SWITCH(NUMSWI),MFIELD,DOCOLL
      COMMON /SWIBLK/MFIELD,DOCOLL,SWITCH
C
C ...  PARAMETERS FOR FERMI BLOCKING ROUTINE
C
      COMMON /FBLBLK/ATEST,RTEST2,PTEST2,EKINMI,LOCRAD,
     *                   RTEST
C
C ...  EQUATION OF STATE PARAMETERS
C
      PARAMETER (IEQINI=1,ISELIN=1)
C                PARABOLA+STRAIGHT LINE SEGM.
      PARAMETER (BEQINI=16.,REQINI=.17,BEQ2IN=21.)
C                BINDING AND EQ. DENSITY
C
C ...  CONSTANTS FOR FORCE AND ACCELERATION CALCULATIONS
C
      PARAMETER (FRAD = 2.)
C                FORCE SPHERE RADIUS
      PARAMETER (FRAD2 = FRAD*FRAD)
      PARAMETER (VOLUME = NSIMUL*4.*PI*FRAD2*FRAD/3.)
      PARAMETER (GRAD = 1./(.5*VOLUME*3.*FRAD/4.))
C
C ...  DERIVATIVE TABLE
C
      PARAMETER (RHOMAX=5.,NTABLE=250)
      COMMON /DVRBLK/DVRTBL(0:NTABLE),DELRHO
C
C ...  MISCELLANEOUS
C
      COMMON /MISC1/SM2CUT(MAXTYP),TIMMAX,XBLCUT
C
C      SM2CUT - LOW ENERGY CUTOFF, USED FOR SIMPLE PAULI BLOCKING SCHEME
C      TIMMAX - TIME AT THE END OF THE CURRENT INTERVAL
C      XBLCUT - COUNTER FOR THE # OF TIMES SM2CUT CAUSED A REJECTION
C
      SAVE
